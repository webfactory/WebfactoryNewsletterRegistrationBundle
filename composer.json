{
    "name": "webfactory/newsletter-registration-bundle",
    "description": "Symfony-Bundle for Newsletter-Registration",
    "license": "MIT",

    "require": {
        "php": ">=7.2",
        "ext-mbstring": "*",
        "doctrine/collections": "^1.6",
        "doctrine/orm": "^2.0",
        "psr/log": "^1.1",
        "ramsey/uuid": "^4.0",
        "symfony/config": "^4.0 | ^5.0",
        "symfony/dependency-injection": "^4.0 | ^5.0",
        "symfony/doctrine-bridge": "^4.0 | ^5.0",
        "symfony/form": "^4.0 | ^5.0",
        "symfony/http-foundation": "^4.0 | ^5.0",
        "symfony/http-kernel": "^2.0 | ^3.0 | ^4.0 | ^5.0",
        "symfony/mailer": "^4.0 | ^5.0",
        "symfony/mime": "^4.0 | ^5.0",
        "symfony/options-resolver": "^4.0 | ^5.0",
        "symfony/routing": "^4.0 | ^5.0",
        "symfony/validator": "^4.0 | ^5.0",
        "twig/twig": "^2.0 | ^3.0"
    },

    "require-dev": {
        "phpunit/phpunit": "^8.5.0",
        "doctrine/common": "^2.0 | ^3.0",
        "doctrine/persistence": "^1.0",
        "webfactory/doctrine-orm-test-infrastructure": "^1.7.0"
    },

    "autoload": {
        "psr-4": {
            "Webfactory\\NewsletterRegistrationBundle\\": "src"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Webfactory\\NewsletterRegistrationBundle\\Tests\\": "tests"
        }
    },

    "config": {
        "sort-packages": true
    }
}
